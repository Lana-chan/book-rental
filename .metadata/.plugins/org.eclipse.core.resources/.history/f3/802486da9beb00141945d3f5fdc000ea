package entidades;
import java.io.Serializable;

public class Exemplar implements Serializable {
	
	private static final long serialVersionUID = 1L;
	private Livro livro;
	private enum disponivel;
	private Usuario proprietario;
	private Usuario[] historicoProprietario;
	private String foto;
	
	public void Exemplar (){
		
	}
	
	public void Exemplar (Livro livro, enum disponivel, Usuario proprietario, Usuario[] historicoProprietario, String foto){
		setLivro(livro);
		setDisponivel(disponivel);
		setProprietario(proprietario);
		setHistoricoProprietario(historicoProprietario);
		setFoto(foto);
	}
	
	public Livro getLivro() {
		return livro;
	}

	public void setLivro(Livro livro) {
		this.livro = livro;
	}
	
	public enum getDisponivel() {
		return disponivel;
	}

	public void setDisponivel(int disponivel) {
		this.disponivel = disponivel;
	}
	
	public Usuario getProprietario() {
		return proprietario;
	}

	public void setProprietario(Usuario proprietario) {
		this.proprietario = proprietario;
	}
	
	public Usuario[] getHistoricoProprietario() {
		return historicoProprietario;
	}

	public void setHistoricoProprietario(Usuario[] proprietario) {
		this.historicoProprietario = historicoProprietario;
	}
	
	public String getFoto() {
		return foto;
	}

	public void setFoto(String foto) {
		this.foto = foto;
	}
	
	
	
	
	
}